{
  "name": "react-full-viewport",
  "version": "0.0.1",
  "description": "A React component that sets its children to full viewport.",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "esm"
  ],
  "author": "Wei Zhu <yesmeck@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yesmeck/react-full-viewport.git"
  },
  "bugs": {
    "url": "https://github.com/yesmeck/react-full-viewport/issues"
  },
  "homepage": "https://github.com/yesmeck/react-full-viewport#readme",
  "scripts": {
    "clean": "rimraf lib esm",
    "prebuild": "npm run clean",
    "build": "npm run build:lib && npm run build:esm",
    "build:lib": "tsc",
    "build:esm": "tsc --module es6 --outDir esm",
    "prepublishOnly": "npm test && npm run build",
    "storybook": "start-storybook -p 9009",
    "build-storybook": "build-storybook",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "release": "standard-version"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@storybook/react": "^4.0.0-alpha.3",
    "@types/react": "^16.4.7",
    "@types/storybook__react": "^3.0.8",
    "awesome-typescript-loader": "^5.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "prettier": "^1.14.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "standard-version": "^4.4.0",
    "typescript": "^3.0.1"
  },
  "peerDependencies": {
    "react": "^0.14.0 || >=15.0.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx,css}": [
      "prettier --write",
      "git add"
    ]
  }
}
